{
  "name": "rccc",
  "version": "0.9.0",
  "private": true,
  "homepage": "https://rosecitychoralcollective.github.io",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.6.0",
    "clsx": "^1.0.4",
    "emailjs-com": "^2.4.1",
    "hookrouter": "^1.2.3",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "react-youtube": "^7.11.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "cleanDeploy": "rm -rf $RC3_DEPLOY/*",
    "copyBuild": "cp -r ./build/* $RC3_DEPLOY",
    "build": "react-scripts build",
    "preDeploy": "yarn run build",
    "deploy": "yarn cleanDeploy && yarn copyBuild && cd $RC3_DEPLOY && git add . && git commit -m \"autoDeploy\" && git push",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js --ext .jsx ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^3.0.0"
  }
}